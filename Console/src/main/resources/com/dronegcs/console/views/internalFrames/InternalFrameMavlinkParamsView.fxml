<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="root" opacity="0.7" prefHeight="${prefHeight}" prefWidth="${prefWidth}" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dronegcs.console.controllers.internalFrames.InternalFrameMavlinkParams">
	<children>
		<VBox>
			<children>
				<HBox alignment="CENTER" spacing="5.0" style="-fx-background-color: ghostwhite">
					<children>
						<Label text="Search" />
						<TextField fx:id="txtSearchField"/>
					</children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
				</HBox>
				<TableView fx:id="table" prefHeight="${root.prefHeight}" prefWidth="1260.0">
					<columns>
						<TableColumn fx:id="id" prefWidth="50.0" text="#" />
						<TableColumn fx:id="name" prefWidth="140.0" text="Name" />
						<TableColumn fx:id="value" prefWidth="140.0" text="Value" />
						<TableColumn fx:id="type" prefWidth="50.0" text="Type" />
						<TableColumn fx:id="update" prefWidth="70.0" text="Update" />
						<TableColumn fx:id="description" prefWidth="1040.0" text="Description" />
					</columns>
				</TableView>
			</children></VBox>
	</children>
</AnchorPane>
